  "bugs": {
    "url": "https://github.com/NicolasOmar/node-practice/issues"
  },
  "homepage": "https://github.com/NicolasOmar/node-practice#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "nodemon": "^2.0.2",
    "yargs": "^15.1.0"
  }
}
