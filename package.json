{
  "name": "node-practice",
  "version": "0.1.0",
  "description": "Used to record practice of nodejs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "notes": "cd ./1-notes && node app.js",
    "note-add": "cd ./1-notes && node app.js add --title=\"Test Title\" --body=\"Body for the test\" ",
    "note-remove": "cd ./1-notes && node app.js remove --title=\"Test Title\"",
    "note-list": "cd ./1-notes && node app.js list",
    "note-read": "cd ./1-notes && node app.js read --title=\"Test Title\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/node-practice.git"
  },
